    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [
          ".txt"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "widgets": [
    {
      "name": "Quick Ride Search",
      "description": "Find a ride quickly.",
      "tag": "search-widget",
      "template": "search-form-template",
      "ms_ac_template": "widgets/search-form.json",
      "data": "widgets/search-data.json",
      "screenshots": [
        {
          "src": "/widgets/search-screenshot.png",
          "sizes": "600x400",
          "type": "image/png"
        }
      ]
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "display_override": [
    "window-controls-overlay"
  ],
  "note_taking": {
    "new_note_url": "/new-note",
    "needs_auth": true
  },
  "scope_extensions": [
    {
      "origin": "*.manakrushi.com"
    }
  ],
  "iarc_rating_id": "",
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+manakrushi",
      "url": "/search?q=%s"
    }
  ],
