    }
  ],
  "screenshots": [
    {
      "src": "https://i.ibb.co/mrqBwfds/IMG-20250920-WA0025.jpg",
      "sizes": "1280x1280",
      "type": "image/jpeg",
      "form_factor": "wide",
      "label": "Mana Krushi Services"
    },
    {
      "src": "https://i.ibb.co/mrqBwfds/IMG-20250920-WA0025.jpg",
      "sizes": "1280x1280",
      "type": "image/jpeg",
      "form_factor": "narrow",
      "label": "Find or Offer Rides"
    }
  ],
  "shortcuts": [
    {
      "name": "Find a Ride",
      "short_name": "Find Ride",
      "description": "Search for available rides to your destination.",
      "url": "/dashboard?role=passenger",
      "icons": [
        {
          "src": "/icons/search.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming and past bookings.",
      "url": "/bookings",
      "icons": [
        {
          "src": "/icons/bookings.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Profile",
      "short_name": "Profile",
      "description": "Update your personal information.",
      "url": "/profile",
      "icons": [
        {
          "src": "/icons/profile.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/search",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [
          ".txt"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "display_override": [
    "window-controls-overlay"
  ],
  "protocol_handlers": [
    {
      "protocol": "web+manakrushi",
      "url": "/search?q=%s"
    }
  ],
  "note_taking": {
    "new_note_url": "/new-note",
    "needs_editing": true
  },
  "iarc_rating_id": "",
  "related_applications": [],
  "prefer_related_applications": false,
  "scope_extensions": [
    {
      "origin": "*.manakrushi.com"
    }
  ],
  "widgets": [
    {
      "name": "Find a Ride",
      "description": "Quickly search for a ride.",
      "tag": "find-ride",
      "template": "find-ride-template",
      "ms_ac_template": "find-ride-adaptive-card",
      "data": "/widgets/find-ride.json",
      "type": "application/json"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  }
}