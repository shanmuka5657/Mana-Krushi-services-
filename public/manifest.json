      "src": "https://i.ibb.co/mrqBwfds/IMG-20250920-WA0025.jpg",
      "sizes": "1080x1080",
      "type": "image/jpeg",
      "form_factor": "wide"
    },
    {
      "src": "https://i.ibb.co/3mSzzSGP/Screenshot-2025-09-14-12-37-47-041-com-whatsapp.jpg",
      "sizes": "1080x2340",
      "type": "image/jpeg",
      "form_factor": "narrow"
    }
  ],
  "share_target": {
    "action": "/search",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Find a Ride",
      "short_name": "Find Ride",
      "description": "Search for available rides",
      "url": "/dashboard?role=passenger",
      "icons": [
        {
          "src": "/logo192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming and past bookings",
      "url": "/bookings",
      "icons": [
        {
          "src": "/logo192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "My Profile",
      "short_name": "Profile",
      "description": "View and edit your profile",
      "url": "/profile",
      "icons": [
        {
          "src": "/logo192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+manakrushi",
      "url": "/search?q=%s"
    }
  ],
  "display_override": [
    "window-controls-overlay"
  ],
  "file_handlers": [
    {
      "action": "/search",
      "accept": {
        "text/plain": [
          ".txt"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "widgets": [
    {
      "name": "My Next Ride",
      "description": "Details about your next scheduled ride",
      "tag": "next-ride",
      "template": "next-ride-template"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "note_taking": {
    "new_note_url": "/dashboard?role=owner",
    "needs_editing_flag": true
  },
  "scope_extensions": [
    {
      "origin": "*.google.com"
    }
  ],
  "iarc_rating_id": "",
  "related_applications": [],
  "prefer_related_applications": false,
  "categories": [
    "travel",
    "business",
    "navigation"
  ],
  "lang": "en",
  "dir": "ltr",
  "orientation": "any"
}