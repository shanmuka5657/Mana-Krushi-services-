{
  "theme_color": "#1E88E5",
  "background_color": "#FFFFFF",
  "display": "standalone",
  "scope": "/",
  "start_url": "/",
  "name": "Mana Krushi Services",
  "short_name": "MK Services",
  "description": "Your partner in shared travel. Find or offer a ride with ease.",
  "screenshots": [
    {
      "src": "https://ibb.co/TD8TS9J",
      "sizes": "1080x2400",
      "type": "image/jpeg",
      "form_factor": "narrow"
    }
  ],
  "categories": [
    "education"
  ],
  "shortcuts": [
    {
      "name": "Find a Ride",
      "short_name": "Find Ride",
      "description": "Search for available rides",
      "url": "/dashboard?role=passenger",
      "icons": [
        {
          "src": "/icons/search.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming and past bookings",
      "url": "/bookings",
      "icons": [
        {
          "src": "/icons/bookings.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+mk",
      "url": "/search?q=%s"
    }
  ],
  "dir": "ltr",
  "lang": "en",
  "orientation": "any",
  "share_target": {
    "action": "/search",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display_override": [
    "standalone"
  ],
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": "auto"
  },
  "id": "/",
  "icons": [
    {
      "src": "https://i.ibb.co/mrqBwfds/IMG-20250920-WA0025.jpg",
      "sizes": "192x192",
      "type": "image/jpeg"
    },
    {
      "src": "data:image/png;base64,",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "data:image/png;base64,",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "data:image/png;base64,",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [
          ".txt"
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "MK Services Widget",
      "description": "Quickly access Mana Krushi Services features.",
      "tag": "mks-widget",
      "template": "mks-widget-template",
      "ms_ac_template": "mks-widget-template",
      "data": "{}"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "note_taking": {
    "new_note_url": "/new-note"
  },
  "scope_extensions": [
    {
      "origin": "*.manakrushi.com"
    }
  ],
  "related_applications": [],
  "iarc_rating_id": "",
  "prefer_related_applications": false
}