{
  "name": "Mana Krushi Services",
  "short_name": "Mana Krushi",
  "description": "Your partner in shared travel. Find or offer a ride with ease.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#1f83bd",
  "orientation": "portrait-primary",
  "id": "/?utm_source=pwa",
  "dir": "ltr",
  "lang": "en-US",
  "scope": "/",
  "display_override": [
    "standalone",
    "minimal-ui"
  ],
  "categories": [
    "travel",
    "social",
    "business"
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "iarc_rating_id": "",
  "launch_handler": {
    "client_mode": [
      "navigate-existing",
      "auto"
    ]
  },
  "scope_extensions": [
    {
      "origin": "*.manakrushi.com"
    }
  ],
  "widgets": [
    {
      "name": "Quick Ride Search",
      "description": "Search for your next ride directly from your home screen.",
      "tag": "search",
      "template": "search_template",
      "ms_ac_template": "widgets/search_template.json",
      "data": "widgets/search_data.json",
      "screenshots": [
        {
          "src": "/screenshots/widget-search.png",
          "sizes": "600x400",
          "type": "image/png",
          "label": "Search Widget"
        }
      ],
      "icons": [
        {
          "src": "https://i.ibb.co/6Rrvs6tM/Whats-App-Image-2025-09-20-at-13-02-57-9dc142ff.png",
          "sizes": "192x192"
        }
      ],
      "auth": false,
      "update": 86400
    }
  ],
  "edge_side_panel": {},
  "note_taking": {},
  "protocol_handlers": [
    {
      "protocol": "web+manakrushi",
      "url": "/search?q=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [
