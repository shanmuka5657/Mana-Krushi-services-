                {
                    "src": "/icons/search.png",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "My Bookings",
            "short_name": "Bookings",
            "description": "View your upcoming and past bookings",
            "url": "/bookings",
            "icons": [
                {
                    "src": "/icons/bookings.png",
                    "sizes": "192x192"
                }
            ]
        }
