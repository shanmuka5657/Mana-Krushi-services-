{
  "name": "Mana Krushi Services",
  "short_name": "ManaKrushi",
  "description": "A ride-sharing and carpooling platform.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#1f83bd",
  "orientation": "portrait-primary",
  "id": "/?utm_source=pwa",
  "display_override": [
    "standalone",
    "window-controls-overlay"
  ],
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-maskable-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icon-maskable-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "https://i.ibb.co/3mhxDmsz/file-00000000c09861fabb32183eb613ae2b.png",
      "type": "image/png",
      "sizes": "1080x2340",
      "form_factor": "narrow",
      "label": "App Screenshot"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.example.app",
      "id": "com.example.app"
    }
  ],
  "prefer_related_applications": false,
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "shortcuts": [
    {
      "name": "Find a Ride",
      "short_name": "Find",
      "description": "Search for available rides",
      "url": "/dashboard?role=passenger",
      "icons": [
        {
          "src": "/shortcut-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Bookings",
      "short_name": "Bookings",
      "description": "View your upcoming and past bookings",
      "url": "/bookings",
      "icons": [
        {
          "src": "/shortcut-icon.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "categories": [
    "travel",
    "navigation"
  ],
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "text/plain": [
          ".txt"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+manakrushi",
      "url": "/search?q=%s"
    }
  ],
  "share_target": {
    "action": "/search",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "widgets": [
    {
      "name": "My Next Ride",
      "description": "View your next upcoming ride.",
      "tag": "next-ride",
      "template": "next-ride-template",
      "ms_ac_template": "next-ride-template.json",
      "data": "/next-ride-data.json",
      "type": "application/json"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "note_taking": {
    "new_note_url": "/new-note",
    "needs_custom_ui": true
  },
  "scope_extensions": [
    {
      "origin": "*.manakrushi.com"
    },
    {
      "origin": "manakrushi.io"
    }
  ]
}