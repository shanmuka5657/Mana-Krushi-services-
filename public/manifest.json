{
  "theme_color": "#1E88E5",
  "background_color": "#FFFFFF",
  "display": "standalone",
  "scope": "/",
  "start_url": "/",
  "id": "/",
  "name": "Mana Krushi Services",
  "short_name": "MK Services",
  "description": "Your partner in shared travel. Find or offer a ride with ease.",
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "data:image/png;base64,",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "/icon-256x256.png",
      "sizes": "256x256",
      "type": "image/png"
    }
  ],
  "lang": "en",
  "dir": "ltr",
  "orientation": "any",
  "prefer_related_applications": false,
  "related_applications": [],
  "share_target": {
    "action": "/search",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "text/*": [
          ".txt"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": [
      "navigate-existing",
      "auto"
    ]
  },
  "widgets": [
    {
      "name": "Recent Bookings",
      "description": "View your recent bookings",
      "tag": "bookings",
      "template": "booking-template",
      "ms_ac_template": "booking-adaptive-card.json",
      "data": "https://example.com/bookings.json",
      "type": "application/json"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "note_taking": {
    "new_note_url": "/new-note",
    "preferred_app": "com.mana-krushi-services.app"
  },
  "scope_extensions": [
    {
      "origin": "*.mana-krushi-services.com"
    }
  ],
  "categories": [
    "education"
  ],
  "shortcuts": [
    {
      "name": "Find a Ride",
      "url": "/dashboard?role=passenger",
      "description": "Quickly search for available rides"
    },
    {
      "name": "My Bookings",
      "url": "/bookings",
      "description": "View your upcoming and past bookings"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+mk",
      "url": "/protocol?type=%s"
    }
  ],
  "iarc_rating_id": ""
}
